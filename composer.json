{
    "name": "likedo/helpers",
    "description": "Help classes commonly used in projects",
    "keywords": [
        "likedo",
        "support",
        "utils",
        "helpers"
    ],
    "type": "library",
    "require": {
        "php": ">=5.4.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    },
    "homepage": "https://github.com/likedo/helpers",
    "license": "MIT",
    "authors": [
        {
            "name": "Francesco Guidotti",
            "email": "francescoguidotti@likedo.it",
            "homepage": "https://www.likedo.it",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Likedo\\Helpers\\": "src"
        },
        "files": [
            "src/ConvertValidateHelper.php",
            "src/DateTimeHelper.php",
            "src/Constants.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Likedo\\Helpers\\Test\\": "test"
        }
    }
}
